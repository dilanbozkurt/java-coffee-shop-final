public class Latte extends CaffeineBeverage {

    public Latte(boolean hasMilk, String size) {
        super(hasMilk, size, "Latte");
    }

    @Override
    public int cost() {
        int cost = 0;
        if (size.equals(Beverage.SMALL))
            cost = 5;
        else if (size.equals(Beverage.MEDIUM))
            cost = 6;
        else if (size.equals(Beverage.LARGE))
            cost = 7;

        if (hasMilk)
            return cost + 1;
        else
            return cost;
    }

}
